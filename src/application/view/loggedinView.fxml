<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<Pane prefHeight="75.0" prefWidth="320.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.controller.LoginController">
   <children>
      <HBox>
         <children>
            <Region />
            <MenuButton fx:id="buttonLoggedin" alignment="CENTER_RIGHT" mnemonicParsing="false" text="Username">
              <items>
                <MenuItem fx:id="buttonSettings" mnemonicParsing="false" text="Settings" />
                <MenuItem fx:id="buttonSignout" mnemonicParsing="false" text="Sign out" />
              </items>
            </MenuButton>
         </children>
         <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </padding>
      </HBox>
   </children>
</Pane>

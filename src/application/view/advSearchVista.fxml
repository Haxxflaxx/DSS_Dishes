<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<Pane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <VBox>
         <children>
            <GridPane>
              <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
              </columnConstraints>
              <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
              </rowConstraints>
            </GridPane>
            <TableView VBox.vgrow="ALWAYS">
              <columns>
                <TableColumn prefWidth="250.0" text="Name" />
                <TableColumn prefWidth="100.0" text="Type" />
                  <TableColumn prefWidth="150.0" text="Cuisine" />
                  <TableColumn prefWidth="100.0" text="Difficulty" />
                  <TableColumn prefWidth="100.0" text="Prep Time" />
              </columns>
            </TableView>
         </children>
      </VBox>
   </children>
</Pane>
